{
"name": "corona extension",
"version":"1.0",
"description":"live corona cases",
"manifest_version":2, 
"content_scripts": [
    {
        "matches":["<all_urls>"],
        "js":["content.js"]
    }
],
"background":{
    "scripts":["background.js"]
},

"icons":{ 
"32":"coronavirus.png" },

"browser_action": {
"default_icon": {
"32":"coronavirus.png" },
"default_popup":"index.html",
"default_title":"coronavirus live status | COVID-19"
}


}